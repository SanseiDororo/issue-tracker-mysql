version: '3.9'

services:
  db:
    container_name: db
    image: mysql:latest
    ports:
      - '3306:3306'
    environment:
      MYSQL_ROOT_PASSWORD: admin
      MYSQL_DATABASE: admin
      MYSQL_USER: admin
      MYSQL_PASSWORD: admin

    volumes:
      - mysql_data:/var/lib/mysql

volumes:
  mysql_data: {}
